{
  //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
  "pages": [
    // #ifdef APP-PLUS || H5
    {
      "path": "pages/hello/index",
      "style": {
        "navigationBarTitleText": "欢迎页"
      }
    },
    // #endif
    {
      "path": "pages/index/index",
      "style": {
        // 透明导航栏
        // #ifdef MP-ALIPAY || APP-PLUS || H5
        "transparentTitle": "auto",
        // #endif
        "navigationBarTitleText": "首页",
        "navigationBarBackgroundColor": "#18905f",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/componentsDemo/index",
      "style": {
        "navigationBarTitleText": "我的组件",
        // 透明导航栏
        // #ifdef MP-ALIPAY || APP-PLUS || H5
        "transparentTitle": "auto",
        // #endif
        "navigationBarBackgroundColor": "#18905f",
        "navigationBarTextStyle": "white"
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/webview/index",
      "style": {
        // "navigationBarTitleText": "webview"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    }
  ],
  // 分包
  "subPackages": [
    // 案例分包
    {
      "root": "pages/case",
      "pages": [
        {
          "path": "case/index",
          "style": {}
        }
      ]
    },
    {
      "root": "pages/p1",
      "pages": [
        {
          "path": "demo/index",
          "style": {}
        }
      ]
    },
    {
      "root": "pages/myPkg",
      "pages": [
        {
          "path": "info/index",
          "style": {}
        },
        {
          "path": "setting/index",
          "style": {}
        }
      ]
    },
    // 组件分包
    {
      "root": "pages/com",
      "pages": [
        {
          "path": "dialog/index",
          "style": {}
        },
        {
          "path": "dragImg/index",
          "style": {}
        },
        {
          "path": "dialogFull/index",
          "style": {}
        },
        {
          "path": "dragList/index",
          "style": {}
        },
        {
          "path": "multiSelect/index",
          "style": {}
        },
        {
          "path": "svgIcon/index",
          "style": {}
        },
        // #ifndef MP
        {
          "path": "poster/index",
          "style": {}
        },
        {
          "path": "copyQrcode/index",
          "style": {}
        },
        // #endif
        {
          "path": "posterWx/index",
          "style": {}
        },
        {
          "path": "form/index",
          "style": {}
        },
        {
          "path": "transition/index",
          "style": {}
        },
        {
          "path": "indexed/index",
          "style": {}
        },
        {
          "path": "editor/index",
          "style": {
            // #ifdef APP-PLUS
            "app-plus": {
              // 控制键盘弹起不上推
              "softinputMode": "adjustResize"
            }
            // #endif
          }
        }
      ]
    }
  ],
  // wifi下预加载
  "preloadRule": {
    "pages/index/index": {
      "network": "wifi",
      "packages": ["pages/p1", "pages/com"]
    }
  },
  // 底部栏
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#18905F",
    "list": [
      {
        "iconPath": "static/img/index.png",
        "selectedIconPath": "static/img/index-a.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/img/activity.png",
        "selectedIconPath": "static/img/activity-a.png",
        "pagePath": "pages/componentsDemo/index",
        "text": "组件"
      },
      {
        "iconPath": "static/img/mien.png",
        "selectedIconPath": "static/img/mien-a.png",
        "pagePath": "pages/my/index",
        "text": "我的"
      }
    ]
  },
  // 权限
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置展示"
    }
  },
  // 小程序内搜索
  "sitemapLocation": "sitemap.json",
  "networkTimeout": {
    "request": 15000,
    "downloadFile": 30000
  },
  "debug": false,
  // 全局样式
  "globalStyle": {
    "backgroundTextStyle": "dark",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTitleText": "",
    "navigationBarTextStyle": "black"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      // "uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "lj-item-(.*)": "lj-components/item/$1/index.vue",
      "lj-(.*)": "lj-components/$1/index.vue"
    }
  }
}
