    {
      "root": "pages/p1",
      "pages": [
        {
          "path": "demo/index",
          "style": {}
        }
      ]
    },
    // 组件分包
    {
      "root": "pages/com",
      "pages": [
        {
          "path": "dialog/index",
          "style": {}
        },
        {
          "path": "dragImg/index",
          "style": {}
        },
        {
          "path": "dialogFull/index",
          "style": {}
        },
        {
          "path": "dragList/index",
          "style": {}
        },
        {
          "path": "multiSelect/index",
          "style": {}
        },
        {
          "path": "svgIcon/index",
          "style": {}
